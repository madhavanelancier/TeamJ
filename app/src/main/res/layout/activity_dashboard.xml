            android:layout_width="wrap_content"
            android:layout_height="200dp"
            android:background="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="0.6"
            android:paddingStart="20dp"
            android:paddingTop="40dp"
            android:paddingEnd="20dp"
            android:paddingBottom="100dp"
            android:scaleType="fitEnd"
            android:src="@drawable/splashlogo"
            app:tint="@color/white"
            card_view:layout_constraintBottom_toBottomOf="@+id/cardView9"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/back"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="16dp"
            android:background="@color/float_transparent"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="@+id/cardView9"
            app:srcCompat="@drawable/ic_back"
            card_view:layout_constraintTop_toTopOf="parent"
            tools:ignore="VectorDrawableCompat" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:background="#4255b3"
            android:visibility="gone"

            app:srcCompat="@drawable/ic_baseline_keyboard_arrow_downwh_24px"
            card_view:layout_constraintBottom_toBottomOf="@+id/back"
            card_view:layout_constraintTop_toTopOf="@+id/back"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <TextView
            android:id="@+id/textView37"
